/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

page{
    height: 100%;
    box-sizing: border-box;
    margin: 0;
    padding: 0;
    font-size: 16px;
}

.no-wrap{
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.title{
    text-align: center;
    font-size: 18px;
}

.cwrap{
    margin: 0 30rpx;
}